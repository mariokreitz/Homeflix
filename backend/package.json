{
  "name": "backend",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon",
    "seed": "node prisma/seed.js",
    "docker:build": "docker compose build",
    "docker:logs": "docker compose logs -f",
    "docker:ps": "docker compose ps",
    "db:start": "docker compose up -d db redis pgadmin",
    "db:stop": "docker compose stop db redis pgadmin",
    "db:init": "docker compose --profile init up db-init",
    "db:reset": "npx prisma migrate reset --force",
    "db:studio": "npx prisma studio",
    "dev:docker": "docker compose up -d db redis pgadmin && npm run dev",
    "prod:build": "docker compose --profile prod build",
    "prod:up": "docker compose --profile prod up -d",
    "prod:down": "docker compose --profile prod down",
    "prod:logs": "docker compose --profile prod logs -f backend"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "bcrypt": "^6.0.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "debug": "~2.6.9",
    "dotenv": "^17.2.0",
    "express": "~4.16.1",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "prisma": "^6.12.0",
    "rate-limit-redis": "^4.2.1",
    "redis": "^5.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
